    font-size: 12px;
    font-family: &quot;Questrial&quot;;
}

QDateEdit::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px;
    border-left: 1px solid rgba(1, 167, 138, 1);  /* Lado izquierdo del botón */
    background-color: rgba(1, 167, 138, 1);
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
}

QDateEdit::up-button, QDateEdit::down-button {
    width: 16px;
    height: 16px;
    border: none;
    background-color: rgba(1, 167, 138, 1);      /* Fondo del botón */
    border-radius: 8px;                           /* Forma circular */
    padding: 0px;
}

QDateEdit::up-arrow {
    image: url(:/icons/up_arrow.svg);            /* Reemplaza con una flecha SVG o PNG hacia arriba */
    width: 10px;
    height: 10px;
}

QDateEdit::down-arrow {
    image: url(:/icons/down_arrow.svg);          /* Reemplaza con una flecha SVG o PNG hacia abajo */
    width: 10px;
    height: 10px;
}

QDateEdit:hover {
